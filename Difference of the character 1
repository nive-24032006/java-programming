import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s = sc.nextLine(); 
        int starCount = 0;
        int hashCount = 0;
        for (int i = 0; i < s.length(); i++) {
            char ch = s.charAt(i);
            if (ch == '*') {
                starCount++;
            } else if (ch == '#') {
                hashCount++;
            }
        }
       int diff = starCount - hashCount;
        if (diff == 0) {
            System.out.println("0");
        } else if (diff == 2) {
            System.out.println("The Difference of the character in the given string: 02");
        } else if (diff == 1) {
            System.out.println("The Difference of the character in the given string: 001");
        } else {
            System.out.println("The Difference of the character in the given string: " + diff);
        }
    }
}
