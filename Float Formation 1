import java.util.Scanner;
public class DesimalDigits{
    public static void main (String[] args){
        Scanner sc = new Scanner(System.in);
        float floatValue = sc.nextFloat();
System.out.printf("%.6f\n", floatValue);
 System.out.printf("%.4f\n", floatValue);
System.out.printf("%.2f\n", floatValue);

  System.out.printf("%.0f\n", floatValue);
}
}
